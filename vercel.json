{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ],
    "crons": [
        {
            "path": "/api/sync-halcon-status",
            "schedule": "*/10 * * * *"
        }
    ]
}